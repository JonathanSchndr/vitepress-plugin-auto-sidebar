{
  "rules": {
    "typescript": {
      "no-explicit-any": "warn",
      "no-unused-vars": "error"
    },
    "eslint": {
      "no-console": "off",
      "no-debugger": "error"
    },
    "import": {
      "no-duplicates": "error"
    }
  },
  "env": {
    "node": true,
    "es2021": true
  },
  "ignorePatterns": ["dist", "node_modules", "coverage", "playground"]
}
