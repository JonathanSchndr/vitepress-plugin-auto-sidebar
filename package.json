  },
  "scripts": {
    "build": "vite build",
    "postbuild": "vue-tsc --emitDeclarationOnly"
  },
  "engines": {
    "node": "^14.13.1 || ^16.7.0 || >=18"
  },
  "keywords": [
    "vitepress",
    "auto",
    "sidebar"
