  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JonathanSchndr/vitepress-plugin-auto-sidebar/issues"
  },
  "homepage": "https://github.com/JonathanSchndr/vitepress-plugin-auto-sidebar#readme",
  "devDependencies": {
    "@types/node": "^18.16.16",
    "vite": "^4.3.9",
    "vue-tsc": "^1.6.5"
  }
